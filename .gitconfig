[user]
  name = beefchimi
  email = curtdulmage@gmail.com
	# These keys are public and safe to expose.
  signingkey = ssh-rsa 
  # Original GPG config:
  # signingkey = 45A002F7F81F19B9
[commit]
  gpgsign = true
[gpg]
  program = gpg
  format = ssh
[gpg "ssh"]
  program = /Applications/1Password.app/Contents/MacOS/op-ssh-sign
[diff]
  algorithm = patience
[protocol]
  version = 2
[core]
  editor = code --wait --new-window
  commitGraph = true
  ignorecase = false
[gc]
  writeCommitGraph = true
[pull]
  ff = only
[credential]
  helper =
  helper = store --file /opt/dev/var/private/git_credential_store
[init]
  defaultBranch = main
